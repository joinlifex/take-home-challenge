{
  "name": "felix-take-home-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll --runInBand",
    "test:coverage": "jest --runInBand --coverage --watchAll=false",
    "start": "nodemon --exec './node_modules/.bin/ts-node -T src/server.ts' --ext ts"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/**.test.+(ts)"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!node-formatter)"
    ],
    "moduleNameMapper": {
      "node-formatter": "<rootDir>/node_modules/node-formatter"
    },
    "watchPathIgnorePatterns": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/server.ts"
    ],
    "coverageProvider": "v8",
    "testEnvironment": "node"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "24.5.1",
    "apollo-server": "2.14.4",
    "class-validator": "0.14.2",
    "date-fns": "^2.15.0",
    "dotenv": "17.2.2",
    "graphql": "16.11.0",
    "reflect-metadata": "0.2.2",
    "sqlite3": "5.1.7",
    "ts-node": "10.9.2",
    "type-graphql": "1.1.1",
    "typeorm": "0.3.26",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@types/date-fns": "2.6.0",
    "@types/jest": "30.0.0",
    "@types/minimatch": "6.0.0",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "babel-jest": "30.1.2",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "6.2.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "3.4.1",
    "jest": "30.1.3",
    "nodemon": "3.1.10",
    "prettier": "1.19.1",
    "ts-jest": "29.4.2",
    "ts-loader": "9.5.4"
  }
}
